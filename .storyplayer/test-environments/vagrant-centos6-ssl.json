{
	"groups":
	[
		{
			"type": "LocalVagrantVms",
			"details": {
				"machines": {
					"default": {
						"osName": "centos6",
						"roles": [
							"upload_target",
							"ssl_target"
						],
						"appSettings": {
							"http": {
								"homepage": "https://storyplayer.test/"
							},
							"user": {
								"username": "vagrant",
								"group": "vagrant"
							}
						}
					}
				}
			},
			"provisioning": {
				"engine": "dsbuild",
				"execute": "dsbuild/vagrant-centos6-ssl.sh"
			}
		}
	],
	"moduleSettings": {
		"http": {
			"validateSsl": false
		}
	}
}