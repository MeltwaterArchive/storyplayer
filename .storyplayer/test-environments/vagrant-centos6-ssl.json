{
	"groups":
	[
		{
			"type": "LocalVagrantVms",
			"details": {
				"machines": {
					"default": {
						"osName": "centos6",
						"roles": [
							"host_target",
							"upload_target",
							"ssl_target",
							"zmq_target"
						],
						"appSettings": {
							"host": {
								"expected": "successfully retrieved this appSetting :)"
							}
						},
						"storySettings": {
							"host": {
								"expected": "successfully retrieved this storySetting :)"
							},
							"http": {
								"homepage": "https://storyplayer.test/"
							},
							"user": {
								"username": "vagrant",
								"group": "vagrant"
							},
							"zmq": {
								"single": {
									"inPort": 5000,
									"outPort": 5001
								},
								"multi": {
									"inPort": 5002,
									"outPort": 5003
								}
							}
						}
					}
				}
			},
			"provisioning": {
				"engine": "dsbuild",
				"execute": "dsbuild/vagrant-centos6-ssl.sh"
			}
		}
	],
	"moduleSettings": {
		"http": {
			"validateSsl": false
		}
	}
}